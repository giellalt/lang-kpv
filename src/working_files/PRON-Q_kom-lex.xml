   <entry>
      <lg>
         <l>душ-мӧд</l>
         <stg>
            <st kpvContlex="QNT">душ-мӧд</st>
         </stg>
      </lg>
      <pos>QNT</pos>
      <e>
         <mg>
            <m>
               <head/>
            </m>
            <tg lang="rus">
               <t>несколько, несколько штук</t>
            </tg>
            <xg>
               <x>душ-мӧд кыв</x>
               <xt lang="rus">несколько слов</xt>
            </xg>
            <xg>
               <x>душ-мӧд морт</x>
               <xt lang="rus">несколько человек</xt>
            </xg>
            <xg>
               <x>душ-мӧдысь кучкыны</x>
