   <entry>
      <lemma>став</lemma>
      <stem/>
      <contlex>PRONQ-STAV</contlex>
      <pos>PRON-Q</pos>
      <e RN="I">
         <mg MNo="1">
            <m>
               <head/>
            </m>
            <tg lang="ru"><t>весь, все, всё</t></tg>
            <xg>
               <x>став му пасьтала</x>
               <xtg lang="ru"><xt>по всей земле, по всему свету</xt></xtg>
               <xtg lang="ru"><xt>по всему полю</xt></xtg>
            </xg>
            <xg>
               <x>став карыс тӧдӧ</x>
               <xtg lang="ru"><xt>весь город знает</xt></xtg>
            </xg>
         </mg>
         <mg MNo="2">
            <m>
               <head/>
            </m>
            <tg lang="ru"><t>все</t><re> (вместе, в полном составе) (употр. тж. с опред.-притяж. суф.)</re></tg>
            <xg>
               <x>ставным</x>
               <xtg lang="ru"><xt>все </xt> <re>мы</re></xtg>
            </xg>
            <xg>
               <x>ставныд</x>
               <xtg lang="ru"><xt>все</xt> <re>вы</re></xtg>
            </xg>
            <xg>
               <x>ставныс</x>
               <xtg lang="ru"><xt>все</xt> <re>они</re></xtg>
            </xg>
            <xg>
               <x>ставным вӧлі тӧдам</x>
               <xtg lang="ru"><xt>мы все знали</xt></xtg>
            </xg>
         </mg>
         <mg MNo="3">
            <m>
               <head><com>в исх. п. в составе сказуемого показывает превосх. ст. качества</com></head>
            </m>
            <tg lang="ru"><t/></tg>
            <xg>
               <x>ставсьыс бур</x>
               <xtg lang="ru"><xt>лучше всех</xt><re>прочих</re></xtg>
               <xtg lang="ru"><xt> лучший из всех</xt></xtg>
            </xg>
         </mg>
         <mg MNo="4">
            <m>
               <head/>
            </m>
            <tg lang="ru"><t>весь, полный, без изъятия, целиком</t></tg>
            <xg>
               <x>став вынсьыс</x>
               <xtg lang="ru"><xt>со всей энергией</xt></xtg>
            </xg>
            <xg>
               <x>няньыс ставыс бырис</x>
               <xtg lang="ru"><xt>хлеб весь вышел</xt></xtg>
            </xg>
         </mg>
         <mg MNo="5">
            <m>
               <head><i>в знач. сущ.</i></head>
            </m>
            <tg lang="ru"><t>все, всех</t></tg>
            <tg lang="ru"><t> всё, что имеется</t></tg>
            <xg>
               <x>быдӧн ӧти вӧсна, ӧти став понда</x>
               <xtg lang="ru"><xt>все за одного, один за всех</xt></xtg>
            </xg>
            <xg>
               <x>ставсӧ победа вылӧ!</x>
               <xtg lang="ru"><xt>всё для победы!</xt></xtg>
            </xg>
            <xg>
               <x>ставсӧ разӧдӧма ва</x>
               <xtg lang="ru"><xt>всё разнесло водой</xt></xtg>
            </xg>
            <xg>
               <x>ставыс бур</x>
               <xtg lang="ru"><xt>всё хорошо</xt></xtg>
            </xg>
            <xg>
               <x>став серти тӧдчӧ</x>
               <xtg lang="ru"><xt>по всему видно</xt></xtg>
            </xg>
            <xg>
               <x>став ловтӧ кыскас</x>
               <xtg lang="ru"><xt>всю душу вымотает</xt><re>надоест своим приставанием</re></xtg>
            </xg>
         </mg>
      </e>
   </entry>
