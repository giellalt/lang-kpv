   <entry>
      <lg>
         <l>быд</l>
         <stg>
            <st kpvContlex="PRONQTAG">быд</st>
         </stg>
      </lg>
      <pos>QNT-PRON</pos>
      <e RNo="I">
         <mg>
            <m>
               <head>
                  <re>мест. обобщ.-опред.</re>
               </head>
            </m>
            <tg lang="rus">
               <t>каждый, всякий, любой</t>
            </tg>
            <xg>
               <x>быд здук</x>
               <xt lang="rus">в любой момент</xt>
            </xg>
            <xg>
               <x>быд лун</x>
               <xt lang="rus">каждый день</xt>
            </xg>
            <xg>
               <x>быд пӧлӧс</x>
               <xt lang="rus">всяких сортов</xt>
            </xg>
            <xg>
               <x>быд нинӧм абуысь видчыны</x>
               <xt lang="rus">ругаться за каждый пустяк</xt>
            </xg>
            <xg>
               <x>быд удж уджалӧ</x>
               <xt lang="rus">любую работу делает</xt>
            </xg>
            <xg>
               <x>тулыс-гожӧмнад быд энь пӧтка быдтысьӧ</x>
               <xt lang="rus">весной и летом каждая птица-самка выводит птенцов</xt>
            </xg>
            <xg genreType="riddle">
               <x>быд керкаын Кузьма да Конӧ</x>
               <xt lang="rus">в каждом доме Кузьма и Конэ</xt>
               <answer>
                  <native>кузь лабич да коник лабич</native>
                  <xt lang="rus">длинная лавка и короткая у входа</xt>
               </answer>
            </xg>
            <xg>
               <x>быд сюв пом эн тырт</x>
               <xt lang="rus">выше живота не ешь</xt>
               <xt lang="rus" tType="literal">каждый конец кишок не заполняй</xt>
            </xg>
         </mg>
      </e>
   </entry>
   <entry>
      <lg>
         <l>быдӧн</l>
         <stg>
            <st kpvContlex="PRONQ-COLL">быдӧн</st>
         </stg>
      </lg>
      <pos>QNT-PRON</pos>
      <e RNo="I">
         <mg No="1">
            <m>
               <head>
                  <re>мест.обобщ.-опред.</re>
               </head>
            </m>
            <tg lang="rus">
               <t>все</t>
            </tg>
            <xg>
               <x>быдӧнлы колӧ сетны</x>
               <xt lang="rus">всем надо дать</xt>
            </xg>
            <xg>
               <x>быдӧнысь видзчысьны</x>
               <xt lang="rus">всех остерегаться</xt>
            </xg>
            <xg>
               <x>быдӧнысь ён</x>
               <xt lang="rus">сильнее всех</xt>
            </xg>
            <xg>
               <x>быдӧнлы пом воӧ</x>
               <xt lang="rus">всем бывает конец</xt>
            </xg>
            <xg genreType="riddle">
               <x>мича-мича ичмонь да быдӧнӧс бӧрдӧдӧ</x>
               <xt lang="rus">молодица-красавица всех заставляет прослезиться</xt>
               <answer>
                  <native>лук</native>
                  <xt lang="rus">лук</xt>
               </answer>
            </xg>
         </mg>
         <mg No="2">
            <m>
               <head/>
            </m>
            <tg lang="rus">
               <t>каждый, всякий</t>
            </tg>
            <xg>
               <x>быдӧн тӧдӧны</x>
               <xt lang="rus">каждый знает</xt>
            </xg>
            <xg>
               <x>быдӧн аслыс кынӧмпӧт корсьӧ</x>
               <xt lang="rus">каждый ищет себе пропитание</xt>
            </xg>
            <xg>
               <x>быдӧнлӧн аслас шог</x>
               <xt lang="rus">у каждого своё горе</xt>
            </xg>
            <xg>
               <x>быдӧн вылӧ эн во</x>
               <xt lang="rus">ко всем не придирайся</xt>
            </xg>
            <xg genreType="proverb">
               <x>быдӧнлысь вомсӧ он каличав</x>
               <xt lang="rus">на каждый роток не накинешь платок</xt>
               <xt lang="rus" tType="literal">у каждого рот на запор не поставишь</xt>
            </xg>
            <xg>
               <x>нянь сёян пиньыд быдӧнлӧн эм</x>
               <xtCtn>
                  <xt lang="rus">чтобы заступиться за себя, зубы у каждого имеются</xt>
                  <re>говорят, когда ссорятся</re>
               </xtCtn>
            </xg>
         </mg>
      </e>
   </entry>
