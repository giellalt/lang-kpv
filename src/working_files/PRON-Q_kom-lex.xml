   <entry>
      <lg>
         <l>став</l>
         <stg>
            <st kpvContlex="PRONQ-STAV">став</st>
         </stg>
      </lg>
      <pos>QNT-PRON</pos>
      <e RNo="I">
         <mg No="1">
            <m>
               <head/>
            </m>
            <tg lang="rus">
               <t>весь, все, всё</t>
            </tg>
            <xg>
               <x>став му пасьтала</x>
               <xt lang="rus">по всей земле, по всему свету</xt>
               <xt lang="rus">по всему полю</xt>
            </xg>
            <xg>
               <x>став карыс тӧдӧ</x>
               <xt lang="rus">весь город знает</xt>
            </xg>
         </mg>
         <mg No="2">
            <m>
               <head/>
            </m>
            <tg lang="rus">
               <tCtn>
                  <t>все</t>
                  <re> (вместе, в полном составе) (употр. тж. с опред.-притяж. суф.)</re>
               </tCtn>
            </tg>
            <xg>
               <x>ставным</x>
               <xtCtn>
                  <xt lang="rus">все</xt>
                  <re>мы</re>
               </xtCtn>
            </xg>
            <xg>
               <x>ставныд</x>
               <xtCtn>
                  <xt lang="rus">все</xt>
                  <re>вы</re>
               </xtCtn>
            </xg>
            <xg>
               <x>ставныс</x>
               <xtCtn>
                  <xt lang="rus">все</xt>
                  <re>они</re>
               </xtCtn>
            </xg>
            <xg>
               <x>ставным вӧлі тӧдам</x>
               <xt lang="rus">мы все знали</xt>
            </xg>
         </mg>
         <mg No="3">
            <m>
               <head>
                  <re>в исх. п. в составе сказуемого показывает превосх. ст. качества</re>
               </head>
            </m>
            <tg lang="rus">
               <t/>
            </tg>
            <xg>
               <x>ставсьыс бур</x>
               <xtCtn>
                  <xt lang="rus">лучше всех</xt>
                  <re>прочих</re>
               </xtCtn>
               <xt lang="rus">лучший из всех</xt>
            </xg>
         </mg>
         <mg No="4">
            <m>
               <head/>
            </m>
            <tg lang="rus">
               <t>весь, полный, без изъятия, целиком</t>
            </tg>
            <xg>
               <x>став вынсьыс</x>
               <xt lang="rus">со всей энергией</xt>
            </xg>
            <xg>
               <x>няньыс ставыс бырис</x>
               <xt lang="rus">хлеб весь вышел</xt>
            </xg>
         </mg>
         <mg No="5">
            <m>
               <head>
                  <re>в знач. сущ.</re>
               </head>
            </m>
            <tg lang="rus">
               <t>все, всех</t>
            </tg>
            <tg lang="rus">
               <t>всё, что имеется</t>
            </tg>
            <xg>
               <x>быдӧн ӧти вӧсна, ӧти став понда</x>
               <xt lang="rus">все за одного, один за всех</xt>
            </xg>
            <xg>
               <x>ставсӧ победа вылӧ!</x>
               <xt lang="rus">всё для победы!</xt>
            </xg>
            <xg>
               <x>ставсӧ разӧдӧма ва</x>
               <xt lang="rus">всё разнесло водой</xt>
            </xg>
            <xg>
               <x>ставыс бур</x>
               <xt lang="rus">всё хорошо</xt>
            </xg>
            <xg>
               <x>став серти тӧдчӧ</x>
               <xt lang="rus">по всему видно</xt>
            </xg>
            <xg>
               <x>став ловтӧ кыскас</x>
               <xtCtn>
                  <xt lang="rus">всю душу вымотает</xt>
                  <re>надоест своим приставанием</re>
               </xtCtn>
            </xg>
         </mg>
      </e>
