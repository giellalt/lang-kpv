LEXICON NUM-CARD_
 CARD ;
:%>ӧд ORD ;
+Err/Dial:%>эд ORD ;
!:%>нан COLL ;

LEXICON CARD
!+Card: ABS_ ; 2018-10-21
+Card: NumCASEPOSSLEX ;
+Card+Der/кӧ+Presentational:%>%-кӧ WordEnd ;
+Card+Der/кӧ+Presentational+Err/Dial:%>%-ке WordEnd ;
!+Ord:%>ӧд NumCASEPOSSLEX ;
+Card+Iter:%>ысь K ; ! нёльысь
+Card+Iter+Ins:%>ысьӧн K ; !нёльысьӧн
+Card+Iter+Ins+Err/Dial:%>ысен K ; !нёльысьӧн
+Distr:%>ӧн PredMonLex ; !нёльӧн
+Distr+Err/Dial:%>эн PredMonLex ; !нёльӧн




LEXICON ORD
+Ord: NumCASEPOSSLEX ; ! ӧд part of the stem
+Ord+Iter:%>ысь K ;
+Ord+Iter+Ins:%>ысьӧн K ;
+Ord+Iter+Ins+Err/Dial:%>ысен K ;
+Ord+Distr:%>ӧн PredMonLex ;
+Ord+Distr+Err/Dial:%>эн PredMonLex ;

LEXICON DET_
!+Det+Ord: ABS_ ; 2018-10-21
+Det+Ord: NumCASEPOSSLEX ; ! ӧд part of the stem
+Det+Ord+Iter:%>ысь K ;
+Det+Ord+Iter+Ins:%>ысьӧн K ;
+Det+Ord+Iter+Ins+Err/Dial:%>ысен K ;
+Det+Card+Distr:%>ӧн K ;
+Det+Card+Distr+Err/Dial:%>эн K ;

LEXICON DET_END
!кыкнан
+AssocColl: # ;


LEXICON NUM-IS_DISTR
+Distr: PredMonLex ;

LEXICON QNT_
+Qnt: NumCASEPOSSLEX ;
