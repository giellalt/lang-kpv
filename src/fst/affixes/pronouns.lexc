!! !!Closed class personal pronouns
! ------------------------------
LEXICON PRONOUN-TYPES
 PERS ;
 INTERR ;
 INDEFINITE-PRON ;
 NEG-PRON ;
 COLL-PRON ;

LEXICON PERS                        !!≈ * __@CODE@__ 
ме+Pron+Pers+Sg1: PERS-SG1 ;    !!≈ * __@CODE@__ ...
тэ+Pron+Pers+Sg2: PERS-SG2 ; 
сійӧ+Pron+Pers+Sg3: PERS-SG3 ;
ме+Pron+Pers+Pl1: PERS-PL1 ;
тэ+Pron+Pers+Pl2: PERS-PL2 ; 
сійӧ+Pron+Pers+Pl3: PERS-PL3 ;


LEXICON PERS-SG1   !!≈ * __@CODE@__ 
+Nom:ме K ;
!+Cmpl:ме WordEnd ; !Postposition complement removed 2019-10-27 same as Nom
+Gen:менам K ;
+Gen+Cop+Pl:менамӧсь K ;
+Gen:мелӧн K ;
+Gen:менамлӧн K ;
+Ela:менсьым K ; !The elative case is used in Sg1 and Sg2 
+Ela+Err/Dial:менчум K ; !The elative case is used in Sg1 and Sg2 
                 !for the functions of the ablative in other persons
+Dat:меным K ; !What should be done with the 4 dative forms?
+Dat+Err/Dial:менум K ;
+Dat:мем K ;
+Dat:мен K ;
+Dat:менлы K ;
+Acc:менӧ K ;
+Use/NG+Err/Dial+Acc:менэ K ;
+Ins:менамӧн K ;
+Com:менамкӧд K ;
+Car:менамтӧг K ;
+Inе:менамын K ;
+Ela:менамысь K ;
+Ill:менамӧ K ;
+Apr:менамлань K ;
+Egr:менамсянь K ;
+Ter:менамӧдз K ;
:ме perssg ; ! for the regular obl sg

LEXICON PERS-PL1    !!≈ * __@CODE@__
+Nom:ми K ;
+Nom:мийӧ K ; !Der/йӧ
+Gen:миян K ;
+Acc:миянӧс K ;
:миян persplobl ;
!!Extras from 2000 Grammar: 163
+Gen:миянлӧн K ; !Synomymous with: миян
+Acc:мияннымӧс K ; !Synomymous with: миянӧс
+Acc+Err/Dial:мияннымес K ; !Synomymous with: миянӧс
+Ela:миянсьыным K ; !Synomymous with:миянлысь
+Ela+Err/Dial:миянсьынум K ; !Synomymous with:миянлысь
+Com:микӧд K ; !Synomymous with: миянкӧд
+Com:мияннымкӧд K ; !Synomymous with: миянкӧд
+Com+Err/Dial:мияннумкед K ; !Synomymous with: миянкӧд
+Car:митӧг K ; !Synomymous with: миянтӧг
+Car:миянтӧгным K ; !Synomymous with: миянтӧг
+Car+Err/Dial:миянтегнум K ; !Synomymous with: миянтӧг
+Cns:мияннымла K ; !Synomymous with: миянла
+Cns+Err/Dial:мияннумла K ; !Synomymous with: миянла
+Ela:миысь K ; !Synomymous with: миянысь
+Ill:миӧ K ; !Synomymous with: миянӧ
+Apr:миянланьным K ; !Synomymous with: миянлань
+Apr:миланьным K ; !Synomymous with: миянлань
+Egr:миянсяньным K ; !Synomymous with: миянсянь
+Apr+Err/Dial:миянланьнум K ; !Synomymous with: миянлань
+Apr+Err/Dial:миланьнум K ; !Synomymous with: миянлань
+Egr+Err/Dial:миянсяньнум K ; !Synomymous with: миянсянь
+Tra:миті K ; !Synomymous with: миянӧд
+Ter:миӧдз K ; !Synomymous with: миянӧдз

LEXICON PERS-SG2     !!≈ * __@CODE@__
+Nom:тэ K ;
!+Cmpl:тэ WordEnd ;  removed 2019-10-27 same as Nom
