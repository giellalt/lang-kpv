<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:element name="att" type="xs:string"/>
<xs:element name="clarif" type="xs:string"/>
<xs:element name="re" type="xs:string"/><!-- restriction -->
<xs:element name="gov" type="xs:string"/>
<xs:element name="CmPos" type="xs:string"/>
<xs:element name="i" type="xs:string"/>
<xs:element name="source" type="xs:string"/>

<xs:element name="register">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="value">
	  <xs:simpleType>
	  <xs:restriction base="xs:string">
	    <xs:enumeration value="kvru"/>
	  </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>


<xs:element name="src">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="value">
	  <xs:simpleType>
	  <xs:restriction base="xs:string">
	    <xs:enumeration value="ERV"/>
	    <xs:enumeration value="ErzDeDict"/>
	    <xs:enumeration value="Erz_2002"/>
	    <xs:enumeration value="VBV"/>
	  </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<!--
<xs:attribute name="contlex">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="ACRON"/>
     <xs:enumeration value="Adj"/>
     <xs:enumeration value="A-CAR"/>
     <xs:enumeration value="Noun1"/>
     <xs:enumeration value="PTC"/>
     <xs:enumeration value="PTC-AN"/>
     <xs:enumeration value="PTC-ANA"/>
     <xs:enumeration value="PTC-OM"/>
     <xs:enumeration value="PTC-TOM"/>
     <xs:enumeration value="Verb1"/>
     <xs:enumeration value="Verb2"/>
     <xs:enumeration value="Verb3"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>
-->

<xs:attribute name="lang">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="en"/>
 <xs:enumeration value="fin"/>
 <xs:enumeration value="kv"/>
 <xs:enumeration value="ru"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>



<xs:attribute name="possPx">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="Sg1"/>
     <xs:enumeration value="Sg2"/>
     <xs:enumeration value="Sg3"/>
     <xs:enumeration value="Pl1"/>
     <xs:enumeration value="Pl2"/>
     <xs:enumeration value="Pl3"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="sem">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="en"/>
 <xs:enumeration value="advl-spat"/>
 <xs:enumeration value="compStand"/><!-- Standard of comparison -->
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="syntax">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="Modifier"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:element name="answer">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
<xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="equivalent">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
<xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="literal">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
<xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="m">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="head"/>
    <xs:choice maxOccurs="unbounded">
    <xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
    <xs:element ref="assoc"/>
    <xs:element ref="literal"/>
    </xs:choice>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="kv">
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="att"/>
    <xs:element ref="CmPos"/>
    <xs:element ref="com"/>
    <xs:element ref="i"/> <!-- Needs to be adapted to specific tags -->
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    </xs:choice>
<xs:attribute ref="pos"/>
  </xs:complexType>
</xs:element>

<xs:element name="stRef">
  <xs:complexType mixed="true">
    <xs:sequence>
    </xs:sequence>
<xs:attribute ref="lang"/>
<xs:attribute ref="pos"/>
<xs:attribute ref="cx"/>
<xs:attribute ref="der"/>
<xs:attribute name="predpx">
 <xs:simpleType>
   <xs:restriction base="xs:string">
    <xs:enumeration value="1Sg"/>
 <xs:enumeration value="2Sg"/>
 <xs:enumeration value="3Sg"/>
 <xs:enumeration value="1Pl"/>
 <xs:enumeration value="2Pl"/>
 <xs:enumeration value="3Pl"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>
<xs:attribute ref="nx"/>
<xs:attribute name="tx">
 <xs:simpleType>
   <xs:restriction base="xs:string">
    <xs:enumeration value="fut"/>
 <xs:enumeration value="pres"/>
 <xs:enumeration value="pretI"/>
 <xs:enumeration value="pretII"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

    </xs:complexType>
</xs:element>

<xs:element name="x">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv"/>
    <xs:element ref="stRef" maxOccurs="unbounded" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="att"/> 
  <xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
    <xs:element ref="assoc"/>
    <xs:element ref="field"/>
    <xs:element ref="sns"/>
    <xs:element ref="source"/>
    <xs:element ref="com"/>
    <xs:element ref="i"/> <!-- This needs to be eliminated by providing a better, informative tag -->
    <xs:element ref="answer"/>
    <xs:element ref="equivalent"/>
    <xs:element ref="literal"/>
    </xs:choice>
    </xs:sequence>
<xs:attribute name="type">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="idiom"/>
     <xs:enumeration value="proverb"/>
     <xs:enumeration value="riddle"/>
     <xs:enumeration value="saying"/>
     <xs:enumeration value="sign"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>
<xs:attribute ref="der"/>
<xs:attribute ref="cx"/>
<xs:attribute ref="possPx"/>
<xs:attribute ref="nx"/>
  </xs:complexType>
</xs:element>

<xs:element name="cxg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="wordForm"/>
    <xs:element ref="head" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
<xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="x"/>
      <xs:element ref="pxg"/>
    </xs:choice>
    </xs:sequence>
<xs:attribute ref="cx"/>
  </xs:complexType>
</xs:element>

<xs:element name="wordForm" type="xs:string"/>

<xs:element name="pxg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="wordForm"/>
    <xs:element ref="head" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
<xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    <xs:element ref="x" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
<xs:attribute ref="possPx"/>
  </xs:complexType>
</xs:element>

<xs:element name="assoc">
  <xs:complexType mixed="true">
    <xs:sequence minOccurs="0">
      <xs:element ref="lemma"/>
      <xs:element ref="stem"/>
      <xs:element ref="contlex" minOccurs="0"/>
      <xs:element ref="pos" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute ref="assocType"/>
    <xs:attribute ref="MNo"/>
    <xs:attribute ref="RN"/>
  </xs:complexType>
</xs:element>

<xs:element name="t"> <!-- translation better yet GLOSS -->
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="att"/>
    <xs:element ref="clarif"/>
    <xs:element ref="com"/>
    <xs:element ref="field"/>
    <xs:element ref="gov"/>
    <xs:element ref="i"/>
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    <xs:element ref="val"/>
    </xs:choice>
     <xs:attribute ref="lang"/>
     <xs:attribute ref="pos"/>
     <xs:attribute ref="sem"/>
     <xs:attribute ref="syntax"/>
    </xs:complexType>
</xs:element>

<xs:element name="te"> <!-- translation description or source word -->
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="att"/>
    <xs:element ref="clarif"/>
    <xs:element ref="com"/>
    <xs:element ref="field"/>
    <xs:element ref="gov"/>
    <xs:element ref="i"/>
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    <xs:element ref="val"/>
    </xs:choice>
     <xs:attribute ref="lang"/>
     <xs:attribute ref="pos"/>
     <xs:attribute ref="sem"/>
     <xs:attribute ref="syntax"/>
    </xs:complexType>
</xs:element>

<xs:element name="tf"> <!-- translation phrase This will be utilized in examples as well -->
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="att"/>
    <xs:element ref="clarif"/>
    <xs:element ref="com"/>
    <xs:element ref="field"/>
    <xs:element ref="gov"/>
    <xs:element ref="i"/>
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    <xs:element ref="val"/>
    </xs:choice>
     <xs:attribute ref="lang"/>
     <xs:attribute ref="pos"/>
     <xs:attribute ref="sem"/>
     <xs:attribute ref="syntax"/>
    </xs:complexType>
</xs:element>

<xs:element name="tg">
  <xs:complexType mixed="true">
    <xs:sequence>
      <xs:choice maxOccurs="unbounded">
    <xs:element ref="t" maxOccurs="unbounded"/>
    <xs:element ref="te" maxOccurs="unbounded"/>
    <xs:element ref="tf" maxOccurs="unbounded"/>
      </xs:choice>
    <xs:element ref="re" maxOccurs="unbounded" minOccurs="0"/>
    <xs:element ref="assoc" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
     <xs:attribute ref="lang"/>
    </xs:complexType>
</xs:element>

<xs:element name="com">
  <xs:complexType mixed="true">
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
<xs:choice minOccurs="0">
  <xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    </xs:sequence>
    <xs:attribute name="kind">
      <xs:simpleType>
        <xs:restriction base="xs:string">
	  <xs:enumeration value="conjugation"/>
	  <xs:enumeration value="declension"/>
	  <xs:enumeration value="derivation"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>


<xs:element name="argg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="arg" maxOccurs="unbounded"/>
    </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="arg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="typical" maxOccurs="unbounded"/>
    </xs:sequence>
     <xs:attribute name="name">
      <xs:simpleType>
	<xs:restriction base="xs:string">
	<xs:enumeration value="obj"/>
 <xs:enumeration value="subj"/>
	</xs:restriction>
       </xs:simpleType>
     </xs:attribute>
     <xs:attribute ref="cx"/>
    </xs:complexType>
</xs:element>

<xs:element name="collg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="typical" maxOccurs="unbounded"/>
    </xs:sequence>
     <xs:attribute name="name">
      <xs:simpleType>
	<xs:restriction base="xs:string">
	<xs:enumeration value="obj"/>
 <xs:enumeration value="subj"/>
	</xs:restriction>
       </xs:simpleType>
     </xs:attribute>
     <xs:attribute ref="cx"/>
    </xs:complexType>
</xs:element>

<xs:element name="field">
  <xs:complexType mixed="true">
    <xs:sequence>
    <xs:element ref="typical" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
     <xs:attribute name="field">
      <xs:simpleType>
	<xs:restriction base="xs:string">
	<xs:enumeration value="AERON"/>
	<xs:enumeration value="AGRIC"/>
	<xs:enumeration value="AMPH"/>
	<xs:enumeration value="ANAT"/>
	<xs:enumeration value="APPAREL"/>
	<xs:enumeration value="ARCH"/>
	<xs:enumeration value="ARCHEOL"/>
	<xs:enumeration value="ASTR"/>
	<xs:enumeration value="BIB"/>
	<xs:enumeration value="BIOL"/>
	<xs:enumeration value="BOT"/>
	<xs:enumeration value="CARDS"/>
	<xs:enumeration value="CARP"/>
	<xs:enumeration value="CELEBR"/>
	<xs:enumeration value="CHEM"/>
	<xs:enumeration value="CHRCH"/>
	<xs:enumeration value="ECON"/>
	<xs:enumeration value="ENT"/>
	<xs:enumeration value="ETHN"/>
	<xs:enumeration value="ETHNOG"/>
	<xs:enumeration value="FAUNA"/>
	<xs:enumeration value="FIN"/>
	<xs:enumeration value="FISH"/>
	<xs:enumeration value="FOLKL"/>
	<xs:enumeration value="FOREST"/>
	<xs:enumeration value="GAME"/>
	<xs:enumeration value="GASTR"/>
	<xs:enumeration value="GEOGR"/>
	<xs:enumeration value="GEOL"/>
	<xs:enumeration value="GRAM"/>
	<xs:enumeration value="HIST"/>
	<xs:enumeration value="HUNT"/>
	<xs:enumeration value="JUR"/>
	<xs:enumeration value="LAW"/>
	<xs:enumeration value="LING"/>
	<xs:enumeration value="LIT"/>
	<xs:enumeration value="MAM"/>
	<xs:enumeration value="MATH"/>
	<xs:enumeration value="MED"/>
	<xs:enumeration value="MIL"/>
	<xs:enumeration value="MIN"/>
	<xs:enumeration value="MUS"/>
	<xs:enumeration value="MYTH"/>
	<xs:enumeration value="ORN"/>
	<xs:enumeration value="PATHOL"/>
	<xs:enumeration value="PHARM"/>
	<xs:enumeration value="PHILOS"/>
	<xs:enumeration value="POLIT"/>
	<xs:enumeration value="PUNCT"/>
	<xs:enumeration value="RELIG"/>
	<xs:enumeration value="REPT"/>
	<xs:enumeration value="SILVA"/>
	<xs:enumeration value="SPORT"/>
	<xs:enumeration value="TECH"/>
	<xs:enumeration value="TEXT"/>
	<xs:enumeration value="THEATR"/>
	<xs:enumeration value="TIME"/>
	<xs:enumeration value="TOOL"/>
	<xs:enumeration value="TOPON"/>
	<xs:enumeration value="VEH"/>
	<xs:enumeration value="ZOOL"/>
	</xs:restriction>
       </xs:simpleType>
     </xs:attribute>
     <xs:attribute ref="cx"/>
    </xs:complexType>
</xs:element>

<xs:element name="range">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="rangeSpat">
	  <xs:simpleType>
	    <xs:restriction base="xs:string">
	      <xs:enumeration value="dialect"/>
	    </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
	<xs:attribute name="rangeTemp">
	  <xs:simpleType>
	    <xs:restriction base="xs:string">
	      <xs:enumeration value="neologism"/>
	      <xs:enumeration value="obsolete"/>
	      <xs:enumeration value="out-dated"/>
	    </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="sns">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="sns">
	  <xs:simpleType>
	    <xs:restriction base="xs:string">
	  <xs:enumeration value="direct"/>
	  <xs:enumeration value="figurative"/>
	  <xs:enumeration value="metaphorical"/>
	</xs:restriction>
       </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="typical">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
<xs:element ref="tg"/>
<!--    <xs:element ref="t"/>
2011-11-08-->
</xs:choice>
    </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="head">
  <xs:complexType mixed="true">
    <xs:choice  maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="argg"/>
    <xs:element ref="asp"/>
    <xs:element ref="att"/>
    <xs:element ref="clarif"/>
    <xs:element ref="collg"/>
    <xs:element ref="com"/>
    <xs:element ref="drv"/>
    <xs:element ref="drvg"/> <!-- This kind of tag should be a node in eg/entry -->
    <xs:element ref="field"/>
    <xs:element ref="i"/>
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    <xs:element ref="val"/> <!-- Needs moving to first "e" node child, perhaps -->
    <xs:element ref="voice"/>
</xs:choice>
    <xs:attribute name="field" type="xs:string"/>
    <xs:attribute name="iter" type="xs:string"/>
		<xs:attribute name="range">
		<xs:simpleType>
		<xs:restriction base="xs:string">
		<xs:enumeration value="dialect"/>
 <xs:enumeration value="neologism"/>
 <xs:enumeration value="obsolete"/>
 <xs:enumeration value="out-dated"/>
		</xs:restriction>
		</xs:simpleType>
		</xs:attribute>
    <xs:attribute name="register" type="xs:string"/>
    <xs:attribute name="sns" type="xs:string"/>
		<xs:attribute name="val">
		<xs:simpleType>
		<xs:restriction base="xs:string">
		<xs:enumeration value="1"/>
		<xs:enumeration value="2"/>
		<xs:enumeration value="3"/>
		<xs:enumeration value="1:S:0"/>
		<xs:enumeration value="2:A:0"/>
		<xs:enumeration value="1,2"/>
		</xs:restriction>
		</xs:simpleType>
		</xs:attribute>
    </xs:complexType>
</xs:element>

<xs:element name="mg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="m"/>
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="x"/>
    <xs:element ref="cxg"/>
    <xs:element ref="assoc"/>
    </xs:choice>
    </xs:sequence>
    <xs:attribute ref="MNo"/>
    </xs:complexType>
</xs:element>


<xs:element name="dict">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="entry" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="lemma"/>
            <xs:element ref="stem"/>
            <xs:element ref="contlex"/>
            <xs:element ref="pos"/>
	    <xs:element ref="drvg" minOccurs="0"/>
	    <xs:element name="e" maxOccurs="unbounded" minOccurs="0">
	      <xs:complexType>
		<xs:sequence>
		  <xs:choice maxOccurs="unbounded" minOccurs="0">
		    <xs:element ref="val" />
		    <xs:element ref="voice" />
		    <xs:element ref="asp" />
		    <xs:element ref="i" />
		    <xs:element ref="att" /> <!-- This should be in mg or even mg/m/head  -->
		  </xs:choice>
		  <xs:element ref="range" minOccurs="0"/> <!-- THIS should be in m/head later -->
		  <xs:element ref="mg" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="RN"/>
	      </xs:complexType>
	    </xs:element>
          </xs:sequence>
	  <xs:attribute name="exclude">
	    <xs:simpleType>
              <xs:restriction base="xs:string">
		<xs:enumeration value="fst"/>
              </xs:restriction>
	    </xs:simpleType>
	  </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="drvg">
  <xs:complexType>
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="drv"/>
      <xs:element ref="drvCtn"/>
    </xs:choice>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:string">
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>

<xs:element name="drvCtn">
  <xs:complexType>
    <xs:sequence>
      <xs:element ref="drv" minOccurs="2" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="composite"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>

<xs:element name="drv">
  <xs:complexType mixed="true">
    <xs:sequence>
      <xs:element ref="lemma" minOccurs="0"/>
      <xs:element ref="stem" minOccurs="0"/>
      <xs:element ref="contlex" minOccurs="0"/>
      <xs:element ref="pos" minOccurs="0"/>
      <xs:element ref="drvMorph" minOccurs="0"/>
      <xs:element ref="com" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute ref="cx"/>
    <xs:attribute ref="der"/>
    <xs:attribute ref="drvType"/>
    <xs:attribute ref="possPx"/>
  </xs:complexType>
</xs:element>

<xs:element name="val"><!-- valency should be inside of the "e" element-->
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="3" minOccurs="0">
      <xs:element ref="syntacticSubject"/>
      <xs:element ref="syntacticObject"/>
      <xs:element ref="com"/>
    </xs:choice>
    <xs:attribute ref="valency"/> <!-- This should have work done on it later  -->
  </xs:complexType>
</xs:element>

<xs:element name="voice" type="xs:string"/> <!-- Need to negotiate with val and valency -->
<xs:element name="asp" type="xs:string"/> <!-- asp = aspect -->

<xs:element name="syntacticSubject">
  <xs:simpleType>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Agent"/>
      <xs:enumeration value="Patient"/>
      <xs:enumeration value="Zero"/>
    </xs:restriction>
  </xs:simpleType>
</xs:element>

<xs:element name="syntacticObject">
  <xs:simpleType>
    <xs:restriction base="xs:string">
