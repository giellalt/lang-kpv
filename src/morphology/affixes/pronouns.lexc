! ========================================================================== !
!!                        !!!Pronominal morphology                           
! ========================================================================== !

!! !!Closed class personal pronouns
! ------------------------------

LEXICON PERS                        !!≈ * __@CODE@__ 
ме+Pron+Pers+Sg1: perssg1decl ;    !!≈ * __@CODE@__ ...
тэ+Pron+Pers+Sg2: perssg2decl ; 
сійӧ+Pron+Pers+Sg3: perssg3decl ;
ми+Pron+Pers+Pl1: perspl1decl ;
ті+Pron+Pers+Pl2: perspl2decl ; 
найӧ+Pron+Pers+Pl3: perspl3decl ;


LEXICON perssg1decl   !!≈ * __@CODE@__ 
+Nom:ме WordEnd ;
+Cmpl:ме WordEnd ; !Postposition complement
+Gen:менам WordEnd ;
+Gen:мелӧн WordEnd ;
+Gen:менамлӧн WordEnd ;
+Ela:менсьым WordEnd ; !The elative case is used in Sg1 and Sg2 
                 !for the functions of the ablative in other persons
+Dat:меным WordEnd ; !What should be done with the 4 dative forms?
+Dat:мем WordEnd ;
+Dat:мен WordEnd ;
+Dat:менлы WordEnd ;
+Acc:менӧ WordEnd ;
+Ins:менамӧн WordEnd ;
+Com:менамкӧд WordEnd ;
+Car:менамтӧг WordEnd ;
+Inе:менамын WordEnd ;
+Ela:менамысь WordEnd ;
+Ill:менамӧ WordEnd ;
+Apr:менамлань WordEnd ;
+Egr:менамсянь WordEnd ;
+Ter:менамӧдз WordEnd ;
:ме perssg ; ! for the regular obl sg

LEXICON perspl1decl    !!≈ * __@CODE@__
+Nom:ми WordEnd ;
+Nom:мийӧ WordEnd ; !Der/йӧ
+Gen:миян WordEnd ;
+Acc:миянӧс WordEnd ;
:миян persplobl ;
!!Extras from 2000 Grammar: 163
+Gen:миянлӧн WordEnd ; !Synomymous with: миян
+Acc:мияннымӧс WordEnd ; !Synomymous with: миянӧс
+Ela:миянсьыным WordEnd ; !Synomymous with:миянлысь
+Com:микӧд WordEnd ; !Synomymous with: миянкӧд
+Com:мияннымкӧд WordEnd ; !Synomymous with: миянкӧд
+Car:митӧг WordEnd ; !Synomymous with: миянтӧг
+Car:миянтӧгным WordEnd ; !Synomymous with: миянтӧг
+Cns:мияннымла WordEnd ; !Synomymous with: миянла
+Ela:миысь WordEnd ; !Synomymous with: миянысь
+Ill:миӧ WordEnd ; !Synomymous with: миянӧ
+Apr:миянланьным WordEnd ; !Synomymous with: миянлань
+Apr:миланьным WordEnd ; !Synomymous with: миянлань
+Egr:миянсяньным WordEnd ; !Synomymous with: миянсянь
+Tra:миті WordEnd ; !Synomymous with: миянӧд
+Ter:миӧдз WordEnd ; !Synomymous with: миянӧдз

LEXICON perssg2decl     !!≈ * __@CODE@__
+Nom:тэ WordEnd ;
+Cmpl:тэ WordEnd ;
+Gen:тэнад WordEnd ;
+Gen:тэлӧн WordEnd ;
+Gen:тэнадлӧн WordEnd ;
+Ela:тэнсьыд WordEnd ;
+Dat:тэныд WordEnd ;
+Dat:тэн WordEnd ;
+Dat:тэд WordEnd ;
+Dat:тэдлы WordEnd ;
+Dat:тэнлы WordEnd ;
+Dat:тэныдлы WordEnd ;
+Acc:тэнӧ WordEnd ;
+Ins:тэнадӧн WordEnd ;
+Com:тэнадкӧд WordEnd ;
+Car:тэнадтӧг WordEnd ;
+Ine:тэнадын WordEnd ;
+Ela:тэнадысь WordEnd ;
+Ill:тэнадӧ WordEnd ;
+Egr:тэнадсянь WordEnd ;
+Ter:тэнадӧдз WordEnd ;
:тэ perssg ;  ! for the regular obl sg

LEXICON perspl2decl !!≈ * __@CODE@__ 
+Nom:ті WordEnd ;
+Cmpl:ті WordEnd ;
+Nom:тійӧ WordEnd ; !We need a special tag for discerning +Nom forms  Der/йӧ
+Gen:тіян WordEnd ;
+Gen:тіянлӧн WordEnd ;
+Acc:тіянӧс WordEnd ;
:тіян persplobl ;
!More forms from Ӧнія коми кыв 2000: 163
+Abl:тіянлысьныд WordEnd ;
+Dat:тіянлы WordEnd ;
+Acc2:тіянныдтӧ WordEnd ;
+Ins:тіӧн WordEnd ;
+Car:тіянныдтӧг WordEnd ;
+Car:тітӧг WordEnd ;
+Cns:тіянныдла WordEnd ;
+Ela:тіысь WordEnd ;
+Ill:тіӧ WordEnd ;
+Apr:тіянланьныд WordEnd ;
+Apr:тіянланьыд WordEnd ;
+Egr:тіянсяньныд WordEnd ;
+Ter:тіӧдз WordEnd ;

LEXICON perssg3decl  !!≈ * __@CODE@__ 
+Nom:сійӧ WordEnd ; !+Der/йӧ
+Gen:сылӧн WordEnd ;
+Abl:сылысь WordEnd ;
+Ela:сыысь WordEnd ;
+Dat:сылы WordEnd ;
+Acc:сійӧс WordEnd ; !Der/йӧ
+Ins:сійӧн WordEnd ; !Der/йӧ
:сы perssg ;  ! for the regular obl sg
+Cmpl:сы WordEnd ;
!More form ӧния коми кыв 163
+Gen:сійӧлӧн WordEnd ; !Der/йӧ
+Abl:сійӧлысь WordEnd ; !Der/йӧ
+Dat:сійӧлы WordEnd ; !Der/йӧ
+Com:сійӧкӧд WordEnd ; !Der/йӧ
+Car:сійӧтӧг WordEnd ; !Der/йӧ
+Apr:сійӧлань WordEnd ; !Der/йӧ
+Egr:сійӧсянь WordEnd ; !Der/йӧ
+Egr:сійӧсяньыс WordEnd ; !Der/йӧ
+Ter:сійӧӧдз WordEnd ; !Der/йӧ
! Special expressions
+Com+Ins:сыкӧдӧн WordEnd ; ! Ен сыкӧдӧн!

!! ми мийӧ The 1st and 2nd persons have Oblique case stem strategies
!! that differ from the 3rd person: 
!! ті тійӧ nämä ovat aivan eri asioita
!! сы сійӧ tosin joskus 

LEXICON perspl3decl !!≈ * __@CODE@__
+Nom:найӧ WordEnd ; !Der/йӧ
+Acc:найӧс WordEnd ; !Der/йӧ
+Gen:налӧн WordEnd ;
:на persplobl ; !This might be one solution for the different stem types
+Cmpl:на WordEnd ;
!
+Gen:найӧлӧн WordEnd ; !Der/йӧ
+Abl:найӧлысь WordEnd ; !Der/йӧ
+Dat:найӧлы WordEnd ; !Der/йӧ
+Ins:найӧӧн WordEnd ; !Der/йӧ
+Com:найӧкӧд WordEnd ; !Der/йӧ
+Com:накӧдыс WordEnd ;
+Car:найӧтӧг WordEnd ; !Der/йӧ
+Car:натӧгыс WordEnd ;
+Cns:наысла WordEnd ;
+Ela:насьыс WordEnd ;
+Apr:наыслань WordEnd ;
+Apr:наланьыс WordEnd ;
+Egr:наыссянь WordEnd ;
+Egr:насяньыс WordEnd ;
+Tra:натіыс WordEnd ;
+Ter:наӧдзыс WordEnd ;


LEXICON perssg !!≈ * __@CODE@__
+Ins:ӧн WordEnd ;
+Com:кӧд WordEnd ;
+Car:тӧг WordEnd ;
+Cns:ла WordEnd ;
+Ine:ын WordEnd ;
+Ill:ӧ WordEnd ; !
+Ela:ысь WordEnd ;
+Apr:лань WordEnd ;
+Egr:сянь WordEnd ;
+Prl:ӧд WordEnd ;
+Tra:ті WordEnd ;
+Ter:ӧдз WordEnd ;
+Abl:лысь WordEnd ; !This is not attested for Sg1 and Sg2 in the 2000 Grammar



LEXICON persplobl !!≈ * __@CODE@__
+Abl:лысь WordEnd ;
+Dat:лы WordEnd ;
+Acc:ӧс WordEnd ;

+Ins:ӧн WordEnd ;
+Com:кӧд WordEnd ;
+Car:тӧг WordEnd ;
+Cns:ла WordEnd ;
+Cmpr:ся WordEnd ;
+Ine:ын WordEnd ;
+Ela:ысь WordEnd ;
+Ill:ӧ WordEnd ;
+Apr:лань WordEnd ;
+Egr:сянь WordEnd ;
+Prl:ӧд WordEnd ;
+Tra:ті WordEnd ;
+Ter:ӧдз WordEnd ;
+AprIne:ланьын WordEnd ;  ! New cases in last grammar  
+AprEla:ланьысь WordEnd ;  ! 
+AprIll:ланьӧ WordEnd ; ! 
+AprEgr:ланьсянь WordEnd ; ! 
+AprPrl:ланьӧд WordEnd ; !
+AprTra:ланьті WordEnd ; !       
+AprTer:ланьӧдз WordEnd ; ! 


