! ========================================================================== !
!!                        !!!Pronominal morphology                           
! ========================================================================== !

!! !!Closed class personal pronouns
! ------------------------------

LEXICON PERS                        !!≈ * __@CODE@__ 
ме+Pron+Pers+Sg1: perssg1decl ;    !!≈ * __@CODE@__ ...
тэ+Pron+Pers+Sg2: perssg2decl ; 
сійӧ+Pron+Pers+Sg3: perssg3decl ;
ми+Pron+Pers+Pl1: perspl1decl ;
ті+Pron+Pers+Pl2: perspl2decl ; 
найӧ+Pron+Pers+Pl3: perspl3decl ;


LEXICON perssg1decl   !!≈ * __@CODE@__ 
+Nom:ме WordEnd ;
+Cmpl:ме WordEnd ; !Postposition complement
+GenStem:менам WordEnd ;
+Cmpl+Gen:мелӧн WordEnd ;
+GenStem+Gen:менамлӧн WordEnd ;
+Ela+PxSg1:менсьым WordEnd ; !The elative case is used in Sg1 and Sg2 
                 !for the functions of the ablative in other persons
+Dat:меным WordEnd ; !What should be done with the 4 dative forms?
+Dat:мем WordEnd ;
+Dat:мен WordEnd ;
+Dat:менлы WordEnd ;
+Acc:менӧ WordEnd ;
+GenStem+Ins:менамӧн WordEnd ;
+GenStem+Com:менамкӧд WordEnd ;
+GenStem+Car:менамтӧг WordEnd ;
+GenStem+Inе:менамын WordEnd ;
+GenStem+Ela:менамысь WordEnd ;
+GenStem+Ill:менамӧ WordEnd ;
+GenStem+Apr:менамсянь WordEnd ;
+GenStem+Ter:менамӧдз WordEnd ;
+Cmpl:ме perssg ; ! for the regular obl sg

LEXICON perspl1decl    !!≈ * __@CODE@__
+Nom:ми WordEnd ;
+Der/йӧ+Nom:мийӧ WordEnd ;
+GenStem:миян WordEnd ;
+Acc:миянӧс WordEnd ;
+GenStem:миян persplobl ;
!!Extras form 2000 Grammar: 163
+GenStem+Gen:миянлӧн WordEnd ; !Synomymous with: миян
+GenStem+PxPl1+Acc:мияннымӧс WordEnd ; !Synomymous with: миянӧс
+GenStem+Ela+PxPl1:миянсьыным WordEnd ; !Synomymous with:миянлысь
+Com:микӧд WordEnd ; !Synomymous with: миянкӧд
+GenStem+PxPl1+Com:мияннымкӧд WordEnd ; !Synomymous with: миянкӧд
+Car:митӧг WordEnd ; !Synomymous with: миянтӧг
+GenStem+Car+PxPl1:миянтӧгным WordEnd ; !Synomymous with: миянтӧг
+GenStem+PxPl1+Cns:мияннымла WordEnd ; !Synomymous with: миянла
+Ela:миысь WordEnd ; !Synomymous with: миянысь
+Ill:миӧ WordEnd ; !Synomymous with: миянӧ
+GenStem+Apr+PxPl1:миянланьным WordEnd ; !Synomymous with: миянлань
+Apr+PxPl1:миланьным WordEnd ; !Synomymous with: миянлань
+GenStem+Egr+PxPl1:миянсяньным WordEnd ; !Synomymous with: миянсянь
+Tra:миті WordEnd ; !Synomymous with: миянӧд
+Ter:миӧдз WordEnd ; !Synomymous with: миянӧдз

LEXICON perssg2decl     !!≈ * __@CODE@__
+Nom:тэ WordEnd ;
+Cmpl:тэ WordEnd ;
+Cmpl+Gen:тэлӧн WordEnd ;
+GenStem:тэнад WordEnd ;
+GenStem+Gen:тэнадлӧн WordEnd ;
+Ela+PxSg2:тэнсьыд WordEnd ;
+Dat:тэныд WordEnd ;
+Dat:тэн WordEnd ;
+Dat:тэд WordEnd ;
+Dat:тэдлы WordEnd ;
+Dat:тэнлы WordEnd ;
+Dat:тэныдлы WordEnd ;
+Acc:тэнӧ WordEnd ;
+GenStem+Ins:тэнадӧн WordEnd ;
+GenStem+Com:тэнакӧд WordEnd ;
+GenStem+Car:тэнатӧг WordEnd ;
+GenStem+Ine:тэнадын WordEnd ;
+GenStem+Ela:тэнадысь WordEnd ;
+GenStem+Ill:тэнадӧ WordEnd ;
+GenStem+Egr:тэнадсянь WordEnd ;
+GenStem+Ter:тэнадӧдз WordEnd ;
+Cmpl:тэ perssg ;  ! for the regular obl sg

LEXICON perspl2decl !!≈ * __@CODE@__ 
+Nom:ті WordEnd ;
+Cmpl:ті WordEnd ;
+Der/йӧ+Nom:тійӧ WordEnd ; !We need a special tag for discerning +Nom forms
+GenStem:тіян WordEnd ;
+GenStem+Gen:тіянлӧн WordEnd ;
+Acc:тіянӧс WordEnd ;
+GenStem:тіян persplobl ;
!More forms from Ӧнія коми кыв 2000: 163
+GenStem+Abl+PxPl2:тіянлысьныд WordEnd ;
+GenStem+Dat:тіянлы WordEnd ;
+GenStem+PxPl2+PxSg2+Acc2:тіянныдтӧ WordEnd ;
+Ins:тіӧн WordEnd ;
+GenStem+PxPl2+Car:тіянныдтӧг WordEnd ;
+Car:тітӧг WordEnd ;
+GenStemPxPl2+Cns:тіянныдла WordEnd ;
+Ela:тіысь WordEnd ;
+Ill:тіӧ WordEnd ;
+GenStem+Apr+PxPl2:тіянланьныд WordEnd ;
+GenStem+Apr+PxSg2:тіянланьыд WordEnd ;
+GenStem+Egr+PxPl2:тіянсяньныд WordEnd ;
+Ter:тіӧдз WordEnd ;

LEXICON perssg3decl  !!≈ * __@CODE@__ 
+Der/йӧ+Nom:сійӧ WordEnd ;
+Cmpl+Gen:сылӧн WordEnd ;
+Cmpl+Abl:сылысь WordEnd ;
+Cmpl+Ela:сыысь WordEnd ;
+Cmpl+Dat:сылы WordEnd ;
+Der/йӧ+Acc:сійӧс WordEnd ;
+Der/йӧ+Ins:сійӧн WordEnd ;
+Cmpl:сы perssg ;  ! for the regular obl sg
+Cmpl:сы WordEnd ;
!More form ӧния коми кыв 163
+Der/йӧ+Gen:сійӧлӧн WordEnd ;
+Der/йӧ+Abl:сійӧлысь WordEnd ;
+Der/йӧ+Dat:сійӧлы WordEnd ;
+Der/йӧ+Com:сійӧкӧд WordEnd ;
+Der/йӧ+Car:сійӧтӧг WordEnd ;
+Der/йӧ+Apr:сійӧлань WordEnd ;
+Der/йӧ+Egr:сійӧсянь WordEnd ;
+Der/йӧ+Egr+PxSg3:сійӧсяньыс WordEnd ;
+Der/йӧ+Ter:сійӧӧдз WordEnd ;
! Special expressions
+Cmpl+Com+Ins:сыкӧдӧн WordEnd ; ! Ен сыкӧдӧн!

!! ми мийӧ The 1st and 2nd persons have Oblique case stem strategies
!! that differ from the 3rd person: 
!! ті тійӧ nämä ovat aivan eri asioita
!! сы сійӧ tosin joskus 

LEXICON perspl3decl !!≈ * __@CODE@__
+Der/йӧ+Nom:найӧ WordEnd ;
+Der/йӧ+Acc:найӧс WordEnd ;
+Cmpl+Gen:налӧн WordEnd ;
+Cmpl:на persplobl ; !This might be one solution for the different stem types
+Cmpl:на WordEnd ;
!
+Der/йӧ+Gen:найӧлӧн WordEnd ;
+Der/йӧ+Abl:найӧлысь WordEnd ;
+Der/йӧ+Dat:найӧлы WordEnd ;
+Der/йӧ+Ins:найӧӧн WordEnd ;
+Der/йӧ+Com:найӧкӧд WordEnd ;
+Cmpl+Com:накӧдыс WordEnd ;
+Der/йӧ+Car:найӧтӧг WordEnd ;
+Cmpl+Car+PxSg3:натӧгыс WordEnd ;
+Cmpl+PxSg3+Cns:наысла WordEnd ;
+Cmpl+Ela+PxSg3:насьыс WordEnd ;
+Cmpl+PxSg3+Apr:наыслань WordEnd ;
+Cmpl+Apr+PxSg3:наланьыс WordEnd ;
+Cmpl+PxSg3+Egr:наыссянь WordEnd ;
+Cmpl+Egr+PxSg3:насяньыс WordEnd ;
+Cmpl+Tra+PxSg3:натіыс WordEnd ;
+Cmpl+Ter+PxSg3:наӧдзыс WordEnd ;


LEXICON perssg !!≈ * __@CODE@__
+Ins:ӧн WordEnd ;
+Com:кӧд WordEnd ;
+Car:тӧг WordEnd ;
+Cns:ла WordEnd ;
+Ine:ын WordEnd ;
+Ill:ӧ WordEnd ; !
+Ela:ысь WordEnd ;
+Apr:лань WordEnd ;
+Egr:сянь WordEnd ;
+Prl:ӧд WordEnd ;
+Tra:ті WordEnd ;
+Ter:ӧдз WordEnd ;
+Abl:лысь WordEnd ; !This is not attested for Sg1 and Sg2 in the 2000 Grammar



LEXICON persplobl !!≈ * __@CODE@__
+Abl:лысь WordEnd ;
+Dat:лы WordEnd ;
+Acc:ӧс WordEnd ;

+Ins:ӧн WordEnd ;
+Com:кӧд WordEnd ;
+Car:тӧг WordEnd ;
+Cns:ла WordEnd ;
+Comp:ся WordEnd ;
+Ine:ын WordEnd ;
+Ela:ысь WordEnd ;
+Ill:ӧ WordEnd ;
+Apr:лань WordEnd ;
+Egr:сянь WordEnd ;
+Prl:ӧд WordEnd ;
+Tra:ті WordEnd ;
+Ter:ӧдз WordEnd ;
+Apr+Ine:ланьын WordEnd ;  ! New cases in last grammar  
+Apr+Ela:ланьысь WordEnd ;  ! 
+Apr+Ill:ланьӧ WordEnd ; ! 
+Apr+Egr:ланьсянь WordEnd ; ! 
+Apr+Prl:ланьӧд WordEnd ; !
+Apr+Tra:ланьті WordEnd ; !       
+Apr+Ter:ланьӧдз WordEnd ; ! 


