LEXICON QNT-CARD_
 CARD ;
:%>ӧд ORD ;
:%>нан COLL ;

LEXICON CARD
+Num+Card: NumCASEPOSSLEX ;
+Num+Card+Der/кӧ+Presentational:%>%-кӧ WordEnd ;
!+Num+Ord:%>ӧд NumCASEPOSSLEX ;
+Qnt+Card+Iter:%>ысь K ;
+Qnt+Card+Iter+Ins:%>ысьӧн K ;
+Qnt+Card+Distr:%>ӧн K ;

LEXICON COLL
+Num+Coll: NumCASEPOSSLEX ; ! _нан_ is already part of the stem
+Num+Coll+Iter:%>ысь K ;
+Num+Coll+Iter+Ins:%>ысьӧн K ;

LEXICON ORD
+Num+Ord: NumCASEPOSSLEX ; ! ӧд part of the stem
+Num+Ord+Iter:%>ысь K ;
+Num+Ord+Iter+Ins:%>ысьӧн K ;

LEXICON NUM-IS_DISTR
+Num+Distr: K ;

LEXICON QNT_
+Qnt: NumCASEPOSSLEX ;
+Qnt+Card+Iter:%>ысь K ;
+Qnt+Card+Iter+Ins:%>ысьӧн K ;
+Qnt+Card+Distr:%>ӧн K ;

LEXICON ITER_
+Qnt+Iter: K ;

!! LEXICON NUM-APPR ! 2011-11-03 This will need work
LEXICON CARD-APPR
+Num+Card+Appr: K ;

LEXICON CARD-ITER-APPR
+Num+Card+Iter+Appr: K ;


