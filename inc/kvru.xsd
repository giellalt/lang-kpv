<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:element name="att" type="xs:string"/>
<xs:element name="clarif" type="xs:string"/>
<xs:element name="gov" type="xs:string"/>
<xs:element name="CmPos" type="xs:string"/>
<xs:element name="i" type="xs:string"/>
<xs:element name="register" type="xs:string"/>

<xs:element name="src">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="value">
	  <xs:simpleType>
	  <xs:restriction base="xs:string">
	    <xs:enumeration value="ERV"/>
	    <xs:enumeration value="ErzDeDict"/>
	    <xs:enumeration value="Erz_2002"/>
	    <xs:enumeration value="VBV"/>
	  </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="stg">
  <xs:complexType>
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="st"/>
      <xs:element ref="stCtn"/>
    </xs:choice>
  </xs:complexType>
</xs:element>

<xs:element name="stem" type="st_Type"/>
<xs:element name="st" type="st_Type"/>

<xs:complexType mixed="true" name="st_Type">
  <xs:sequence>
    <xs:element ref="src" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
  <xs:attribute ref="contlex"/>
  <xs:attribute ref="cx"/>
  <xs:attribute ref="nx"/>
  <xs:attribute ref="pos"/>
  <xs:attribute ref="possNx"/>
  <xs:attribute ref="semNx"/>
</xs:complexType>


<xs:element name="stCtn">
  <xs:complexType>
    <xs:sequence>
      <xs:element ref="st" minOccurs="2" maxOccurs="unbounded"/>
      <xs:element ref="src" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="ws-separated"/>
          <xs:enumeration value="hyphenated"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>




<xs:attribute name="contlex">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="ACRON"/>
     <xs:enumeration value="Adj"/>
     <xs:enumeration value="A-CAR"/>
     <xs:enumeration value="Noun1"/>
     <xs:enumeration value="PTC"/>
     <xs:enumeration value="PTC-AN"/>
     <xs:enumeration value="PTC-ANA"/>
     <xs:enumeration value="PTC-OM"/>
     <xs:enumeration value="PTC-TOM"/>
     <xs:enumeration value="Verb1"/>
     <xs:enumeration value="Verb2"/>
     <xs:enumeration value="Verb3"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="lang">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="en"/>
 <xs:enumeration value="fin"/>
 <xs:enumeration value="ru"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="pos">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="A"/>
     <xs:enumeration value="A-CAR"/>
     <xs:enumeration value="ADV"/>
     <xs:enumeration value="AP"/>
     <xs:enumeration value="COMPL"/>
     <xs:enumeration value="N"/>
     <xs:enumeration value="N-DV"/>
     <xs:enumeration value="nn"/>
     <xs:enumeration value="phrase-v"/>
     <xs:enumeration value="PRON-PERS"/>
     <xs:enumeration value="PRON-REFL"/>
     <xs:enumeration value="PRON-REL"/>
     <xs:enumeration value="PTC"/>
     <xs:enumeration value="V"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="possPx">
 <xs:simpleType>
   <xs:restriction base="xs:string">
    <xs:enumeration value="sg1"/>
 <xs:enumeration value="sg2"/>
 <xs:enumeration value="sg3"/>
 <xs:enumeration value="pl1"/>
 <xs:enumeration value="pl2"/>
 <xs:enumeration value="pl3"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="syntax">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="Modifier"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:element name="answer">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:element ref="t" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="equivalent">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:element ref="t" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="literal">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:element ref="t" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="m">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="head"/>
    <xs:choice maxOccurs="unbounded">
    <xs:element ref="t"/>
    <xs:element ref="assoc"/>
    <xs:element ref="literal"/>
    </xs:choice>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="kv">
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="att"/>
    <xs:element ref="CmPos"/>
    <xs:element ref="com"/>
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    </xs:choice>
  </xs:complexType>
</xs:element>

<xs:element name="stRef">
  <xs:complexType>
    <xs:sequence>
    </xs:sequence>
<xs:attribute ref="lang"/>
<xs:attribute ref="pos"/>
<xs:attribute ref="cx"/>
<xs:attribute ref="der"/>
<xs:attribute name="predpx">
 <xs:simpleType>
   <xs:restriction base="xs:string">
    <xs:enumeration value="1Sg"/>
 <xs:enumeration value="2Sg"/>
 <xs:enumeration value="3Sg"/>
 <xs:enumeration value="1Pl"/>
 <xs:enumeration value="2Pl"/>
 <xs:enumeration value="3Pl"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>
<xs:attribute ref="nx"/>
<xs:attribute name="tx">
 <xs:simpleType>
   <xs:restriction base="xs:string">
    <xs:enumeration value="fut"/>
 <xs:enumeration value="pres"/>
 <xs:enumeration value="pretI"/>
 <xs:enumeration value="pretII"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

    </xs:complexType>
</xs:element>

<xs:element name="x">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv"/>
    <xs:element ref="stRef" maxOccurs="unbounded" minOccurs="0"/>
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="att"/>
    <xs:element ref="t"/>
    <xs:element ref="field"/>
    <xs:element ref="answer"/>
    <xs:element ref="com"/>
    <xs:element ref="literal"/>
    <xs:element ref="equivalent"/>
    </xs:choice>
    </xs:sequence>
<xs:attribute name="type">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="proverb"/>
     <xs:enumeration value="riddle"/>
     <xs:enumeration value="saying"/>
     <xs:enumeration value="sign"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>
<xs:attribute ref="der"/>
<xs:attribute ref="cx"/>
<xs:attribute ref="possPx"/>
<xs:attribute ref="nx"/>
  </xs:complexType>
</xs:element>

<xs:element name="assoc">
  <xs:complexType mixed="true">
    <xs:sequence>
      <xs:element ref="lg"/>
      <xs:choice>
	<xs:element ref="stg"/>
	<xs:element ref="stem"/>
      </xs:choice>
    </xs:sequence>
    <xs:attribute ref="assocType"/>
    <xs:attribute ref="MNo"/>
    <xs:attribute ref="RN"/>
  </xs:complexType>
</xs:element>

