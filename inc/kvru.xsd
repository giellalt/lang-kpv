<xs:element name="t">
  <xs:complexType mixed="true">
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="att"/>
    <xs:element ref="clarif"/>
    <xs:element ref="com"/>
    <xs:element ref="field"/>
    <xs:element ref="gov"/>
    <xs:element ref="i"/>
    <xs:element ref="range"/>
    <xs:element ref="register"/>
    <xs:element ref="sns"/>
    </xs:choice>
     <xs:attribute ref="lang"/>
     <xs:attribute ref="pos"/>
     <xs:attribute ref="syntax"/>
    </xs:complexType>
</xs:element>

<xs:element name="com">
  <xs:complexType mixed="true">
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:element ref="t" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="kind">
      <xs:simpleType>
        <xs:restriction base="xs:string">
	  <xs:enumeration value="conjugation"/>
	  <xs:enumeration value="declension"/>
	  <xs:enumeration value="derivation"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>


<!--
<xs:element name="collg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="coll" maxOccurs="unbounded"/>
    </xs:sequence>
    </xs:complexType>
</xs:element>
-->

<xs:element name="argg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="arg" maxOccurs="unbounded"/>
    </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="arg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="typical" maxOccurs="unbounded"/>
    </xs:sequence>
     <xs:attribute name="name">
      <xs:simpleType>
	<xs:restriction base="xs:string">
	<xs:enumeration value="obj"/>
 <xs:enumeration value="subj"/>
	</xs:restriction>
       </xs:simpleType>
     </xs:attribute>
     <xs:attribute ref="cx"/>
    </xs:complexType>
</xs:element>

<xs:element name="collg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="typical" maxOccurs="unbounded"/>
    </xs:sequence>
     <xs:attribute name="name">
      <xs:simpleType>
	<xs:restriction base="xs:string">
	<xs:enumeration value="obj"/>
 <xs:enumeration value="subj"/>
	</xs:restriction>
       </xs:simpleType>
     </xs:attribute>
     <xs:attribute ref="cx"/>
    </xs:complexType>
</xs:element>

<xs:element name="field">
  <xs:complexType mixed="true">
    <xs:sequence>
    <xs:element ref="typical" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
     <xs:attribute name="field">
      <xs:simpleType>
	<xs:restriction base="xs:string">
	<xs:enumeration value="AERON"/>
	<xs:enumeration value="AGRIC"/>
	<xs:enumeration value="AMPH"/>
	<xs:enumeration value="ANAT"/>
	<xs:enumeration value="APPAREL"/>
	<xs:enumeration value="ARCH"/>
	<xs:enumeration value="ARCHEOL"/>
	<xs:enumeration value="ASTR"/>
	<xs:enumeration value="BIB"/>
	<xs:enumeration value="BIOL"/>
	<xs:enumeration value="BOT"/>
	<xs:enumeration value="CARDS"/>
	<xs:enumeration value="CARP"/>
	<xs:enumeration value="CELEBR"/>
	<xs:enumeration value="CHEM"/>
	<xs:enumeration value="CHRCH"/>
	<xs:enumeration value="ECON"/>
	<xs:enumeration value="ENT"/>
	<xs:enumeration value="ETHN"/>
	<xs:enumeration value="ETHNOG"/>
	<xs:enumeration value="FAUNA"/>
	<xs:enumeration value="FIN"/>
	<xs:enumeration value="FISH"/>
	<xs:enumeration value="FOLKL"/>
	<xs:enumeration value="FOREST"/>
	<xs:enumeration value="GAME"/>
	<xs:enumeration value="GASTR"/>
	<xs:enumeration value="GEOGR"/>
	<xs:enumeration value="GEOL"/>
	<xs:enumeration value="GRAM"/>
	<xs:enumeration value="HIST"/>
	<xs:enumeration value="HUNT"/>
	<xs:enumeration value="JUR"/>
	<xs:enumeration value="LAW"/>
	<xs:enumeration value="LING"/>
	<xs:enumeration value="LIT"/>
	<xs:enumeration value="MAM"/>
	<xs:enumeration value="MATH"/>
	<xs:enumeration value="MED"/>
	<xs:enumeration value="MIL"/>
	<xs:enumeration value="MIN"/>
	<xs:enumeration value="MUS"/>
	<xs:enumeration value="MYTH"/>
	<xs:enumeration value="ORN"/>
	<xs:enumeration value="PATHOL"/>
	<xs:enumeration value="PHARM"/>
	<xs:enumeration value="PHILOS"/>
	<xs:enumeration value="POLIT"/>
	<xs:enumeration value="PUNCT"/>
	<xs:enumeration value="RELIG"/>
	<xs:enumeration value="REPT"/>
	<xs:enumeration value="SILVA"/>
	<xs:enumeration value="SPORT"/>
	<xs:enumeration value="TECH"/>
	<xs:enumeration value="TEXT"/>
	<xs:enumeration value="THEATR"/>
	<xs:enumeration value="TIME"/>
	<xs:enumeration value="TOOL"/>
	<xs:enumeration value="TOPON"/>
	<xs:enumeration value="VEH"/>
	<xs:enumeration value="ZOOL"/>
	</xs:restriction>
       </xs:simpleType>
     </xs:attribute>
     <xs:attribute ref="cx"/>
    </xs:complexType>
</xs:element>

<xs:element name="range">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="rangeSpat">
	  <xs:simpleType>
	    <xs:restriction base="xs:string">
	      <xs:enumeration value="dialect"/>
	    </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
	<xs:attribute name="rangeTemp">
	  <xs:simpleType>
	    <xs:restriction base="xs:string">
	      <xs:enumeration value="neologism"/>
	      <xs:enumeration value="obsolete"/>
	    </xs:restriction>
	  </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="sns">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute name="sns">
	  <xs:simpleType>
	    <xs:restriction base="xs:string">
	  <xs:enumeration value="direct"/>
	  <xs:enumeration value="figurative"/>
	  <xs:enumeration value="metaphorical"/>
	</xs:restriction>
       </xs:simpleType>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:element name="typical">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="kv" minOccurs="0"/>
    <xs:element ref="t" maxOccurs="unbounded"/>
    </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="head">
  <xs:complexType mixed="true">
    <xs:choice  maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="argg"/>
    <xs:element ref="att"/>
    <xs:element ref="clarif"/>
    <xs:element ref="collg"/>
    <xs:element ref="com"/>
    <xs:element ref="drv"/>
    <xs:element ref="field"/>
    <xs:element ref="range"/>
    <xs:element ref="sns"/>
</xs:choice>
    <xs:attribute name="field" type="xs:string"/>
    <xs:attribute name="iter" type="xs:string"/>
		<xs:attribute name="range">
		<xs:simpleType>
		<xs:restriction base="xs:string">
		<xs:enumeration value="dialect"/>
 <xs:enumeration value="neologism"/>
 <xs:enumeration value="obsolete"/>
		</xs:restriction>
		</xs:simpleType>
		</xs:attribute>
    <xs:attribute name="register" type="xs:string"/>
    <xs:attribute name="sns" type="xs:string"/>
		<xs:attribute name="val">
		<xs:simpleType>
		<xs:restriction base="xs:string">
		<xs:enumeration value="1"/>
		<xs:enumeration value="2"/>
		<xs:enumeration value="3"/>
		</xs:restriction>
		</xs:simpleType>
		</xs:attribute>
    </xs:complexType>
</xs:element>

<xs:element name="mg">
  <xs:complexType>
    <xs:sequence>
    <xs:element ref="m"/>
    <xs:choice maxOccurs="unbounded" minOccurs="0">
    <xs:element ref="x"/>
    <xs:element ref="assoc"/>
    </xs:choice>
    </xs:sequence>
    <xs:attribute ref="MNo"/>
    </xs:complexType>
</xs:element>


<xs:element name="r">
  <xs:complexType>
    <xs:sequence>
      <xs:element name="eg" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="lg"/>
	    <xs:choice>
              <xs:element ref="stem"/>
              <xs:element ref="stg"/>
	    </xs:choice>
	    <xs:element ref="drvg" minOccurs="0"/>
	    <xs:element name="e" maxOccurs="unbounded">
	      <xs:complexType>
		<xs:sequence>
		  <xs:element ref="range" minOccurs="0"/> <!-- THIS should be in m/head later -->
		  <xs:element ref="mg" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="RN"/>
	      </xs:complexType>
	    </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="drvg">
  <xs:complexType>
    <xs:choice maxOccurs="unbounded">
      <xs:element ref="drv"/>
      <xs:element ref="drvCtn"/>
    </xs:choice>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:string">
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>

<xs:element name="drvCtn">
  <xs:complexType>
    <xs:sequence>
      <xs:element ref="drv" minOccurs="2" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="composite"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
</xs:element>

<xs:element name="drv">
  <xs:complexType>
    <xs:sequence>
      <xs:element ref="lg"/>
      <xs:element ref="stg"/>
      <xs:element ref="com" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute ref="contlex"/>
    <xs:attribute ref="der"/>
    <xs:attribute ref="drvType"/>
  </xs:complexType>
</xs:element>
<!-- ATTRIBUTES -->

<xs:attribute name="assocType">
  <xs:simpleType>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ant"/>
      <xs:enumeration value="compare"/>
      <xs:enumeration value="hyponym"/>
      <xs:enumeration value="hypernym"/>
      <xs:enumeration value="meronym"/>
      <xs:enumeration value="ruSeeKv"/>
      <xs:enumeration value="seeKv"/>
      <xs:enumeration value="see"/>
      <xs:enumeration value="seeAlso"/>
      <xs:enumeration value="syn"/>
    </xs:restriction>
  </xs:simpleType>
</xs:attribute>


<xs:attribute name="cx">
  <xs:simpleType>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Dat"/>
      <xs:enumeration value="Ela"/>
      <xs:enumeration value="Gen"/>
      <xs:enumeration value="Ill"/>
      <xs:enumeration value="Ine"/>
      <xs:enumeration value="Instr"/>
      <xs:enumeration value="Nom"/>
    </xs:restriction>
  </xs:simpleType>
</xs:attribute>

<xs:attribute name="der">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="ан"/>
     <xs:enumeration value="ана"/>
     <xs:enumeration value="ігкежлӧ"/>
     <xs:enumeration value="іг"/>
     <xs:enumeration value="ігтыр"/>
     <xs:enumeration value="ігтырйи"/>
     <xs:enumeration value="ӧм"/>
     <xs:enumeration value="тӧг"/>
     <xs:enumeration value="тӧдз"/>
     <xs:enumeration value="тӧм"/>
     <xs:enumeration value="ысь"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:attribute name="drvType">
 <xs:simpleType>
   <xs:restriction base="xs:string">
     <xs:enumeration value="Derан"/>
     <xs:enumeration value="Derана"/>
     <xs:enumeration value="Derігкежлӧ"/>
     <xs:enumeration value="Derіг"/>
     <xs:enumeration value="Derігтыр"/>
     <xs:enumeration value="Derігтырйи"/>
     <xs:enumeration value="Derӧм"/>
     <xs:enumeration value="Derтӧг"/>
     <xs:enumeration value="Derтӧдз"/>
     <xs:enumeration value="Derтӧм"/>
     <xs:enumeration value="Derысь"/>
     <xs:enumeration value="DerИн"/>
     <xs:enumeration value="Descriptive"/>
     <xs:enumeration value="Dimторй"/>
     <xs:enumeration value="ONOM"/>
     <xs:enumeration value="PTC"/>
     <xs:enumeration value="PTC-AN"/>
     <xs:enumeration value="PTC-OM"/>
   </xs:restriction>
 </xs:simpleType>
</xs:attribute>

<xs:element name="lg">
  <xs:complexType>
    <xs:sequence>
      <xs:element ref="l"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="l">
  <xs:complexType>
    <xs:simpleContent>
      <xs:extension base="xs:string">
	<xs:attribute ref="contlex"/>
	<xs:attribute ref="pos"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:element>

<xs:attribute name="MNo">
  <xs:simpleType>
    <xs:restriction base="xs:string">
      <xs:enumeration value="1"/>
      <xs:enumeration value="2"/>
      <xs:enumeration value="3"/>
      <xs:enumeration value="4"/>
      <xs:enumeration value="5"/>
      <xs:enumeration value="6"/>
      <xs:enumeration value="7"/>
      <xs:enumeration value="8"/>
      <xs:enumeration value="9"/>
